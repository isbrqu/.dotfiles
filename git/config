# Este es el archivo de configuraci√≥n de Git por usuario.
[alias]
	sts = status
	cmt = commit
	uns = reset HEAD --
	unw = checkout --
	lod = log --oneline --decorate
	co = checkout
[core]
	excludesfile = ~/.config/dot/git/ignore-global

[color]
	pager = true
	ui = always
	interactive = always
	status = always
	branch = always
	diff = always
	grep = always
	decorate = always
	showbranch = always
